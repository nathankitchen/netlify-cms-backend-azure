# config.yml changes needed to connect to Azure DevOps

backend:
  name: azure
  branch: master
  repo: <organisation>/<project>/<repo>
  identity_url: https://login.microsoftonline.com/<your-tenant-id>
  app_id: <your-app-id>
  api_root: https://dev.azure.com/<devops-org-name>
  
site_url: "http://localhost:8080"